	"semanticHighlighting": true,
	"semanticTokenColors": {
		"string": "#d98dc7",
		"comment": "#546262",
		"punctuation": "#e0dad7",
		"*.attribute": "#e0dad7",
		"property": "#e0dad7",
		"*.controlFlow": "#e0dad7",
		"parameter": "#e0dad7",
		"variable": "#e0dad7",
		"*.mutable": {
			"bold": true,
			"foreground": "#c3c3da"
		},
		"macro": "#ED9E8D",
		"function": {
			"foreground": "#ED9E8D",
			"bold": true
		},
		"method": "#ED9E8D",
		"*.callable": "#ED9E8D",
		"*.consuming": {
			"bold": true
		},
		"builtinType": "#B74413",
		"decorator": "#B74413",
		"deriveHelper": "#B74413",
		"type": "#EC7643",
		"typeAlias": "#EC7643",
		"typeParameter": "#EC7643",
		"struct": "#EC7643",
		"enum": "#EC7643",
		"selfTypeKeyword": {
			"bold": true,
			"foreground": "#EC7643"
		},
		"*.constant": "#E0E32B",
		"lifetime": "#E0E32B",
		"number": "#E0E32B",
		"enumMember": "#E0E32B",
		"interface": "#E0E32B",
		"formatSpecifier": "#E0E32B",
		"keyword": "#55d9c1",
		"operator": "#55d9c1",
		"*.deprecated": {
			"strikethrough": true
		}
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#546262"
			}
		},
		{
			"scope": "keyword.operator.key-value",
			"settings": {
				"foreground": "#e0dad7"
			}
		},
		{
			"scope": "string,entity.other.attribute-name.class",
			"settings": {
				"foreground": "#d98dc7"
			}
		},
		{
			"scope": "punctuation.separator,keyword,storage.modifier,storage.type,punctuation.accessor,punctuation.colon,heading",
			"settings": {
				"foreground": "#55d9c1"
			}
		},
		{
			"scope": "support.type.primitive",
			"settings": {
				"foreground": "#B74413"
			}
		},
		{
			"scope": "support.class,entity.name.type,support.type,entity.name.tag,markup.bold.markdown,meta.link.inline.markdown",
			"settings": {
				"foreground": "#EC7643"
			}
		},
		{
			"scope": "variable.other.metavariable.specifier,entity.name.type.result,constant.character.enum,variable.other.constant.property,constant,support.constant,entity.other.attribute-name.pseudo-element,variable.parameter.graphql,punctuation.definition.list.begin",
			"settings": {
				"foreground": "#E0E32B"
			}
		},
		{
			"scope": "entity.name.function,support.function,markup.inline,variable.language.function",
			"settings": {
				"foreground": "#ED9E8D"
			}
		}
	]
}
